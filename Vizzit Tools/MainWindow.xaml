<Window x:Class="Vizzit_Tools.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Vizzit Tools" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="DomainTextBox" HorizontalAlignment="Left" Height="23" Margin="70,29,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBlock x:Name="DomainTextBlock" HorizontalAlignment="Left" Margin="10,30,0,0" TextWrapping="Wrap" Text="Domain" VerticalAlignment="Top"/>
        <Button x:Name="StartButton" Content="Crawl" HorizontalAlignment="Left" Margin="204,29,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-0.08,-0.15" Height="23" Click="StartButton_Click"/>
        <TextBlock x:Name="DebugTextBlock" HorizontalAlignment="Left" Margin="10,63,0,0" TextWrapping="Wrap" Text="Debug" VerticalAlignment="Top" Height="246" Width="497"/>

    </Grid>
</Window>
